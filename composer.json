{
    "name": "adminetic/contact",
    "description": "Contact Module for Admientic Admin Panel",
    "keywords": [
        "adminetic",
        "contact"
    ],
    "homepage": "https://github.com/adminetic/contact",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pratik Shrestha",
            "email": "pratikdai404@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Adminetic\\Contact\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adminetic\\Contact\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adminetic\\Contact\\Provider\\ContactServiceProvider"
            ]
        }
    }
}
